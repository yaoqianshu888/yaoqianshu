	<bean id="nodeSearchAction" class="com.landicorp.dms.action.node.NodeSearchAction"
		scope="prototype" />
	<bean id="nodeDetailAction" class="com.landicorp.dms.action.node.NodeDetailAction"
		scope="prototype" />
	<bean id="nodeInsertAction" class="com.landicorp.dms.action.node.NodeInsertAction"
		scope="prototype" />
	<bean id="nodeUpdateAction" class="com.landicorp.dms.action.node.NodeUpdateAction"
		scope="prototype" />
	<bean id="nodeViewAction" class="com.landicorp.dms.action.node.NodeViewAction"
		scope="prototype" />
	<bean id="nodeDeleteAction" class="com.landicorp.dms.action.node.NodeDeleteAction"
		scope="prototype" />
