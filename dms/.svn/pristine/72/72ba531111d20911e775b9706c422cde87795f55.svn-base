	<bean id="documentViewAction" class="com.landicorp.dms.action.document.DocumentViewAction"
		scope="prototype" />
	<bean id="documentMenuAction" class="com.landicorp.dms.action.document.DocumentMenuAction"
		scope="prototype" />
	<bean id="documentContentAction" class="com.landicorp.dms.action.document.DocumentContentAction"
		scope="prototype" />
