	<bean id="functionVersionSearchAction" class="com.landicorp.dms.action.functionVersion.FunctionVersionSearchAction"
		scope="prototype" />
	<bean id="functionVersionDetailAction" class="com.landicorp.dms.action.functionVersion.FunctionVersionDetailAction"
		scope="prototype" />
	<bean id="functionVersionCompareAction" class="com.landicorp.dms.action.functionVersion.FunctionVersionCompareAction"
		scope="prototype" />
		<!-- 文档版本 -->
	<bean id="documentVersionSearchAction" class="com.landicorp.dms.action.documentVersion.DocumentVersionSearchAction"
		scope="prototype" />
	<bean id="documentVersionViewAction" class="com.landicorp.dms.action.documentVersion.DocumentVersionViewAction"
