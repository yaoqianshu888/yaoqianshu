package com.landicorp.upms.action.area;

import java.util.List;

import com.landicorp.core.action.BaseActionSupport;
import com.landicorp.core.entities.Area;

public class AreaSearchAction extends BaseActionSupport {
	
	private static final long serialVersionUID = 1L;
	private Area area;
	private Area parentArea;
	private Integer superAreaId;

	public String searchAreaBySuperAreaId() {
		return "areaSearch";
	}
	
	public String search(){
		return "areaSearch";
	}

	public String searchAreaForAreaTree() {
		return "areaTree";
	}

	public String getAreaDetail() {
		return "areaDetail";
	}

	public Integer getSuperAreaId() {
		return superAreaId;
	}

	public void setSuperAreaId(Integer superAreaId) {
		this.superAreaId = superAreaId;
	}

	public Area getArea() {
		return area;
	}

	public void setArea(Area area) {
		this.area = area;
	}


	public Area getParentArea() {
		return parentArea;
	}

	public void setParentArea(Area parentArea) {
		this.parentArea = parentArea;
	}

}
